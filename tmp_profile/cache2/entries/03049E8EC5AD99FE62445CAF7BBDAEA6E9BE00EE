        playThock();
        const nextChar = text[input.length];
        if (nextChar && e.key !== nextChar) {
          setShake((prev) => prev + 1);
        }
      }
      handleKeyDown(e);
    };
    const handleKeyUp = (e) => {
      if (e.getModifierState("CapsLock")) setCapsLock(true);
      else
        setCapsLock(false);
    };
