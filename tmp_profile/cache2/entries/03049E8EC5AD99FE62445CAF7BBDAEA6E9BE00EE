      if (e.getModifierState("CapsLock")) setCapsLock(true);
      else
        setCapsLock(false);
      if (!isFocused) return;
      if (e.key === "Tab" || isFinished && e.key === "Enter") {
        e.preventDefault();
        reset();
        return;
      }
      if (e.key === "Escape") {
        e.preventDefault();
        reset();
        return;
      }
