  });
  const zenMode = isRunning && !isFinished;
  const reset = () => {
    setGhostIndex(0);
    engineReset();
  };
  useEffect(() => {
    if (isRunning && lastStats && lastStats.wpm > 0) {
      const charPerSec = lastStats.wpm * 5 / 60;
      const intervalMs = 1e3 / charPerSec;
      const interval = setInterval(() => {
        setGhostIndex((prev) => {
          if (prev < text.length) return prev + 1;
          return prev;
        });
